{
  "name": "<%= packageName %>",
  "version": "0.1.0",
  "description": "<%= appDescription %>",
  "main": "src/index.js",
  "repository": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "stripescli serve",
    "build": "stripescli build --output ./output",
    "test": "stripescli test --run demo --show",
    "lint": "eslint src"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.8.0",
    "@folio/eslint-config-stripes": "^1.1.0"
  },
  "dependencies": {
    "@folio/stripes-components": "^2.0.0",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-router-dom": "^4.1.1"
  },
  "stripes": {
    "type": "app",
    "displayName": "<%= displayName %>",
    "route": "<%= appRoute %>",
    "hasSettings": true,
    "okapiInterfaces": {},
    "permissionSets": [
      {
        "permissionName": "module.<%= appName %>.enabled",
        "displayName": "UI: <%= displayName %> module is enabled",
        "visible": true
      },
      {
        "permissionName": "settings.<%= appName %>.enabled",
        "displayName": "Settings (<%= appName %>): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": true
      }
    ]
  }
}
